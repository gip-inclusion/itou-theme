  </head>

  <body id="top" class="l-not-authenticated">
    <nav role="navigation" aria-label="Accès rapide" tabindex="0" class="visually-hidden">
      <ul>
        <li><a href="#nav-primary">Aller au menu principal</a></li>
        <li><a href="#main">Aller au contenu principal</a></li>
      </ul>
    </nav>

    <%- include('_sections/header') %>

      <main class="s-main" role="main">

        <section class="s-title-02">
          <div class="s-title-02__container container">
            <div class="s-title-02__row row">
              <div class="s-title-02__col col-12">
                <div class="c-prevstep">
                  <a href="/search/employers" class="btn btn-ico btn-link ps-0" aria-label="Retour à la page précédente">
                    <i class="ri-arrow-drop-left-line ri-xl font-weight-medium" aria-hidden="true"></i>
                    <span>
                      Retour
                    </span>
                  </a>
                </div>
                <h1>Rechercher un emploi inclusif</h1>
                <form class="mt-md-4">
                  <div class="row">
                    <div class="col-12">
                      <label for="id_city" class="visually-hidden">Ville</label>
                      <div class="input-group input-group-xl input-group-search flex-nowrap">
                        <div class="input-group-text bg-white rounded-start ps-3 pe-2">
                          <i class="ri-map-pin-line ri-lg text-disabled" aria-hidden="true"></i>
                        </div>
                        <select name="city" lang="fr" data-minimum-input-length="2" data-theme="bootstrap-5" data-allow-clear="false" class="form-control django-select2 select2-hidden-accessible" data-ajax--url="/autocomplete/cities?select2=&amp;slug=" data-placeholder="Rechercher un emploi inclusif autour de…" required="" id="id_city" data-select2-id="id_city" tabindex="-1" aria-hidden="true">
                          <option value="paris-75" selected="" data-select2-id="2">Paris (75)</option>

                        </select><span class="select2 select2-container select2-container--bootstrap-5" dir="ltr" data-select2-id="1" style="width: 515.467px;"><span class="selection"><span class="select2-selection select2-selection--single" role="combobox" aria-haspopup="true" aria-expanded="false" tabindex="0" aria-disabled="false" aria-labelledby="select2-id_city-container"><span class="select2-selection__rendered" id="select2-id_city-container" role="textbox" aria-readonly="true" title="Paris (75)">Paris (75)</span><span class="select2-selection__arrow" role="presentation"><b role="presentation"></b></span></span></span><span class="dropdown-wrapper" aria-hidden="true"></span></span>
                        <div class="input-group-text bg-white p-0">
                          <button class="btn btn-ico btn-link" aria-label="Rechercher un emploi inclusif">
                            <i class="ri-search-line font-weight-bold" aria-hidden="true"></i>
                            <span>Rechercher</span>
                          </button>
                        </div>
                      </div>
                    </div>
                  </div>
                </form>
                <ul id="job-search-tabs" class="s-tabs-01__nav nav nav-tabs mt-3 mt-md-4 mb-0">
                  <li class="nav-item">
                    <a class="nav-link active" data-matomo-event="true" data-matomo-category="candidature" data-matomo-action="clic" data-matomo-option="clic-onglet-employeur" href="/search/employers/results?city=paris-75&amp;city_name=Paris+%2875%29&amp;distance=25">
                      <i class="ri-hotel-line font-weight-normal me-1" aria-hidden="true"></i>
                      <span>Employeurs</span>
                      <span class="badge badge-sm rounded-pill ms-2">598</span>
                    </a>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link" data-matomo-event="true" data-matomo-category="candidature" data-matomo-action="clic" data-matomo-option="clic-onglet-fichesdeposte" href="/search/job-descriptions/results?city=paris-75&amp;city_name=Paris+%2875%29&amp;distance=25">
                      <i class="ri-briefcase-4-line font-weight-normal me-1" aria-hidden="true"></i>
                      <span>Postes <span class="d-none d-md-inline">ouverts au recrutement</span></span>
                      <span class="badge badge-sm rounded-pill ms-2">1254</span>
                    </a>
                  </li>
                </ul>
              </div>
            </div>
          </div>
        </section>


      </main>

      <%- include('_sections/footer') %>

        <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js" integrity="sha384-BBtl+eGJRgqQAUMxJ7pMwbEyER4l1g+O15P+16Ep7Q9Q+zqX6gSbd85u4mG4QzX+" crossorigin="anonymous"></script>
        <script src="javascripts/app.js"></script>
  </body>

</html>
